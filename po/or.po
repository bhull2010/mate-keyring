#: ../daemon/data/mate-keyring.schemas.in.h:1
#, fuzzy
#| msgid ""
#| "This option enables the PKCS#11 component in the mate-keyring daemon. It "
#| "only takes effect as mate-keyring-daemon starts, (ie: when the user logs "
